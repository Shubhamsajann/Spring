package com.telusko.main.dao;

import org.springframework.stereotype.Repository;

@Repository  // Marks this class as a Spring-managed bean of type "Repository"
// Bean ID is automatically "dao" (lowercase class name by default)
public class Dao {

    public Dao() {
        // Constructor prints message when bean is created by Spring
        System.out.println("Dao Bean Created");
    }

}
